<dashboard>
  <label>Pineapple Mapping</label>
  <row>
    <panel>
      <title>SSID Mapping</title>
      <viz type="leaflet_maps_app.maps-plus">
        <title>SSID data from wigle.net</title>
        <search>
          <query>source="ssid_results_*.json" index="pineapple" sourcetype="_json" | eval tooltip=ssid | rename trilat as latitude, trilong as longitude | eval description=strftime(_time,"%m/%d/%y %H:%M:%S")| table latitude, longitude, tooltip, description</query>
          <earliest>0</earliest>
          <latest></latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="drilldown">none</option>
        <option name="leaflet_maps_app.maps-plus.allPopups">1</option>
        <option name="leaflet_maps_app.maps-plus.animate">1</option>
        <option name="leaflet_maps_app.maps-plus.autoFitAndZoom">1</option>
        <option name="leaflet_maps_app.maps-plus.autoFitAndZoomDelay">500</option>
        <option name="leaflet_maps_app.maps-plus.bingMaps">0</option>
